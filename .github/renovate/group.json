{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "packages=[various k8s pkgs] | group k8s pkg updates by version",
      "groupName": "k8s-version-{{newVersion}}",
      "matchPackageNames": [
        "kindest/node",
        "kubernetes/kubernetes"
      ]
    }
  ]
}
